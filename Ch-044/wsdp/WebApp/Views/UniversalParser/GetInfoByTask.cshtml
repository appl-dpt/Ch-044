@model List<Model.DTO.ExecutingInfoDTO>
@{
	ViewBag.Title = "GetInfoByTaskId";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Get Info By Task Id</h2>

<table id="executeinfo" class="table table-striped table-bordered" cellspacing="0" width="100%">
	<thead>
		<tr>
			<th>Id</th>
			<th>Status</th>
			<th>Parser Task Id</th>
			<th>Good Url</th>
			<th>Error Message</th>
			<th>Date</th>
		</tr>
	</thead>
	<tbody>
	@foreach (var item in Model) {
		<tr>
			<td>@item.Id</td>
			<td>@item.Status.ToString()</td>
			<td>@item.ParserTaskId</td>
			<td>@item.GoodUrl</td>
			<td>@item.ErrorMessage</td>
			<td>@item.Date.ToString("dd/MM/yyyy")</td>
		</tr>
	}
	</tbody>

	<tfoot>
		<tr>
			<th>Id</th>
			<th>Status</th>
			<th>Parser Task Id</th>
			<th>Good Url</th>
			<th>Error Message</th>
			<th>Date</th>
		</tr>
	</tfoot>
</table>
